<img class="avatar js-action-profile-avatar tweet-user-avatar" 
    data-user-rank="{{rank}}" 
    data-user-ff="{{isFF}}" 
    data-user-tweets="{{tweets}}"
    data-user-likes="{{likes}}"
    data-user-follows="{{follows}}"
    data-user-id="{{id}}"
    data-user-name="{{name}}"
    data-user-screen-name="{{screenName}}"
    title="{{name}}"
    rel="tooltip" 
    data-toggle="tooltip"
    src="{{profileImage}}" alt="{{name}}" />
